#include<bits/stdc++.h>
using namespace std;


int Sum_array_element(int arr[],int n){

if(n<0){
        return 0;
}

return arr[n] +Sum_array_element(arr,n-1);}

int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];

    }
    cout<<"Sum of the array elements:"<<Sum_array_element(arr,n)<<endl; //output

    return 0;


}
