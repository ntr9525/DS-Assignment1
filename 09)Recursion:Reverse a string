#include<bits/stdc++.h>
using namespace std;

void reverse_str(char str[],int i)
{

    if(i<0)
    {
        return;
    }

    cout<<str[i];
    return reverse_str(str,i-1);

}


int main()
{

    char str[100];
    cin>>str;

    int len=strlen(str);


    reverse_str(str,len-1);


    return 0;
}
